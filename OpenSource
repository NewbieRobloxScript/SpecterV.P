local Misc = Window:NewTab("Misc")
local MiscSection = Misc:NewSection("Misc")

MiscSection:NewToggle("Ghost ESP", "ESP for the ghost.", function(state)
        if state then
            local Ghost = game.Workspace.GhostModel.HumanoidRootPart
            local QuestMarker = Instance.new("BillboardGui")
            local ImageLabel = Instance.new("ImageLabel")
            QuestMarker.Name = "GhostESP"
            QuestMarker.Parent = Ghost
            QuestMarker.AlwaysOnTop = true
            QuestMarker.Size = UDim2.new(0, 50, 0, 50)
            ImageLabel.Parent = QuestMarker
            ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ImageLabel.BackgroundTransparency = 1.000
            ImageLabel.Size = UDim2.new(1, 0, 1, 0)
            ImageLabel.Image = "rbxassetid://2334137638"
            ImageLabel.ImageColor3 = Color3.fromRGB(0, 0, 255)

        else
            game:GetService("Workspace").GhostModel.HumanoidRootPart.GhostESP:Destroy()
        end
    end)



MiscSection:NewButton("Full Bright", "Make your game at full brightness.", function()
game.Lighting.Brightness = 5
game.Lighting.ClockTime = 9
game.Lighting.GlobalShadows = false
end)

MiscSection:NewToggle("Noclip", "Make you go through objects!", function(state)
    getgenv().clip = state
    game:GetService("RunService").Stepped:Connect(function()
        if getgenv().clip then
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
        end
    end)
end)


MiscSection:NewSlider("WalkSpeed", "Changes your walkspeed.", 250, 16, function(v)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
end)

MiscSection:NewSlider("JumpPower", "Changes your jumppower.", 250, 16, function(v)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
end)


MiscSection:NewButton("Orb Checking!", "Orb Check", function()
  pcall(function()
    while true do
        local Orb = workspace:FindFirstChild('Orb') or workspace('Orb')
        if Orb then
            warn("SCRIPT - :\nOrb")
        end
        task.wait(2)
    end
    print('pcall print')
end)
end)


MiscSection:NewButton("Ghost Visible!", "u can see ghost block!", function()
local ghost = game:GetService("Workspace").GhostModel
ghost.HumanoidRootPart.Transparency = 0
ghost.HumanoidRootPart.Changed:Connect(function()
   ghost.HumanoidRootPart.Transparency = 0
end)
end)


MiscSection:NewButton("Sanity Check", "use /console to see!", function()
for i,v in pairs(game.Players:GetChildren()) do
   if v.Data:FindFirstChild("Sanity") then
       print(v.Name .. " - " .. tostring(v.Data.Sanity.Value))
   end
end
end)



    MiscSection:NewToggle("Always Alone", "Toggle whether you are always alone or not.", function(state)
        if state then
            game.Players.LocalPlayer.Data.Alone.Value = true

        else
            game.Players.LocalPlayer.Data.Alone.Value = false

        end
    end)
    MiscSection:NewToggle("Always Outside", "Toggle whether you are always outside or not.", function(state)
        if state then
            game.Players.LocalPlayer.Data.Outside.Value = true

        else
            game.Players.LocalPlayer.Data.Outside.Value = false

        end
    end)



MiscSection:NewButton("Remove Doors", "Remove all doors on the map!", function()
    game.Workspace.House.Outside.Doors:ClearAllChildren()
game:GetService("Workspace").House.Doors:ClearAllChildren()
     end)







