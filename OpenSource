function NotificationCallback(text)
		if text == "Yes" then

            local requestfunc = syn and syn.request or http and http.request or http_request or fluxus and fluxus.request or getgenv().request or request
    
    
            spawn(function()
                    for i = 1, 14 do
                        spawn(function()
                            local reqbody = {
                                ["nonce"] = game:GetService("HttpService"):GenerateGUID(false),
                                ["args"] = {
                                    ["invite"] = {["kJzgxpGReg"] = "kJzgxpGReg"},
                                    ["code"] = "kJzgxpGReg",
                                },
                                ["cmd"] = "INVITE_BROWSER"
                            }
                            local newreq = game:GetService("HttpService"):JSONEncode(reqbody)
                            requestfunc({
                                Headers = {
                                    ["Content-Type"] = "application/json",
                                    ["Origin"] = "https://discord.com"
                                },
                                Url = "http://127.0.0.1:64"..(53 + i).."/rpc?v=1",
                                Method = "POST",
                                Body = newreq
                            })
                        end)
                    end
                end)



			setclipboard("https://discord.gg/kJzgxpGReg")
			toclipboard("https://discord.gg/kJzgxpGReg")
		else
		print("")
		end
	end
	local callback = Instance.new("BindableFunction",script)
	callback.OnInvoke = NotificationCallback
	game.StarterGui:SetCore("SendNotification",  {
		Title = "Discord Invite";
		Text = "Would you like to join my discord?";
		Icon = "rbxassetid://6816665736";
		Duration = 10;
		Callback = callback;
		Button1 = "Yes";
		Button2 = "No";
	})


local supported_game = {
	 [5911084042] = {
		  5921569465
   },
     [4483381587] = {
		  4483381587
	 }
}

local function get_alt_id(id)
  for i,v in pairs(supported_game) do  
	  if (table.find(v, id) ~= nil) then 
		  return true
	           end
	      end
	    return false
	 end
	  print(get_alt_id(5921569465))


local sound = Instance.new("Sound", game.Workspace)
            sound.SoundId = "rbxassetid://6169705721"
            sound.Looped = false
            sound.Volume = 100
            sound:Play()
            wait(0.1)





